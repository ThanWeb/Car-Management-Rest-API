            "get": {
                "tags": [
                    "Car"
                ],
                "responses": {
                    "200": {
                        "description": "List of cars",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "SUCCESS"
                                        },
                                        "cars": {
                                            "type": "array",
                                            "items": {
                                            "type": "object",
                                                "properties": {
                                                    "name": {
                                                        "type": "string",
                                                        "example": "Toyota Avanza"
                                                    },
                                                    "type": {
                                                        "type": "string",
                                                        "example": "Family"
                                                    },
                                                    "size": {
                                                        "type": "string",
                                                        "example": "Medium"
                                                    },
                                                    "rentPerDay": {
                                                        "type": "integer",
                                                        "example": 150000
                                                    }
                                                }
                                            }
                                        },
                                        "meta": {
                                            "type": "object",
                                            "properties": {
                                                "total": {
                                                    "type": "integer",
                                                    "example": 1
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Error list of cars",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "FAILED"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Error Message"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
